{
  "name": "zskarte-v3",
  "version": "3.0.0-alpha.0",
  "main": "dist/electron/index.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "ng:serve": "ng serve --port 4300",
    "ng:serve:dev": "ng serve --port 4300 --host 0.0.0.0 --configuration development-local",
    "ng:serve:local": "ng serve --port 4300 --configuration local",
    "ng:serve:swerder": "ng serve --port 4300 --configuration swerder",
    "ng:serve:e2e": "npm-run-all -p ng:serve:dev wait",
    "ng:serve:prod": "ng serve --port 4300 --configuration production",
    "ng:build:prod": "ng build --configuration production",
    "ng:build:dev": "ng build --configuration development",
    "electron:serve": "yarn electron:build && wait-on http-get://localhost:4300/ && electron ./ --serve",
    "electron:build": "tsc -p tsconfig.electron.json && electron-builder install-app-deps",
    "build": "run-p ng:build:prod electron:build",
    "start": "run-p ng:serve:dev electron:serve",
    "start:local": "run-p ng:serve:local electron:serve",
    "lint": "eslint \"src/**/*.ts\" --max-warnings=0",
    "prettify": "prettier --write \"**/*.{js,ts,html,css,json,md}\"",
    "test:e2e": "playwright test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.9",
    "@angular/cdk": "17.0.5",
    "@angular/common": "^17.0.9",
