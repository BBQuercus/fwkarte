    "@angular/material": "17.0.5",
    "@angular/platform-browser": "^17.0.9",
    "@angular/platform-browser-dynamic": "^17.0.9",
    "@angular/pwa": "^17.0.10",
    "@angular/router": "^17.0.9",
    "@angular/service-worker": "^17.0.9",
    "@types/file-saver": "^2.0.5",
    "@types/geojson": "^7946.0.14",
    "dexie": "^4.0.8",
    "exceljs": "^4.3.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "immer": "^10.1.1",
    "jspdf": "^2.5.2",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
